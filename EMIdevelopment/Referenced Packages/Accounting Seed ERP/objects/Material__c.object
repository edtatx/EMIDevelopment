    </validationRules>
    <validationRules>
        <fullName>Quantity_Cannot_Be_Changed_If_Allocated</fullName>
        <active>true</active>
        <description>the quantity per unit cannot be changed if inventory movements have been allocated to the material</description>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp; PRIORVALUE( Quantity_Per_Unit__c ) != Quantity_Per_Unit__c &amp;&amp; Quantity_Allocated__c &gt; 0</errorConditionFormula>
        <errorDisplayField>Quantity_Per_Unit__c</errorDisplayField>
        <errorMessage>You cannot change the quantity per unit if the quantity allocated is greater than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_Per_Unit_Positive_Value</fullName>
        <active>true</active>
        <description>quantity needed should not be less than zero</description>
        <errorConditionFormula>Quantity_Per_Unit__c  &lt; 0</errorConditionFormula>
        <errorDisplayField>Quantity_Per_Unit__c</errorDisplayField>
        <errorMessage>Value must be greater than or equal to 0</errorMessage>
    </validationRules>
