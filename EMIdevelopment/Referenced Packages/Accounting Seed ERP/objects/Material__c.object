    <validationRules>
        <fullName>Product_Cannot_Be_Changed_If_Allocated</fullName>
        <active>true</active>
        <description>the product cannot be changed if inventory movements have been allocated to the material</description>
        <errorConditionFormula>NOT(ISNEW()) &amp;&amp; PRIORVALUE(Product__c) != Product__c &amp;&amp;  Quantity_Allocated__c &gt; 0</errorConditionFormula>
        <errorDisplayField>Product__c</errorDisplayField>
        <errorMessage>You cannot change the product if the quantity allocated is greater than 0</errorMessage>
    </validationRules>
    <validationRules>
