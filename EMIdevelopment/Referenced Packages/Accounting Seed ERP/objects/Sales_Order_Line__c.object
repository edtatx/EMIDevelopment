    <validationRules>
        <fullName>Allocated_Greater_Equal_Shipped</fullName>
        <active>true</active>
        <description>Ensures the quantity allocated is always greater than or equal to the quantity shipped</description>
        <errorConditionFormula>Quantity_Shipped__c &gt;  Quantity_Allocated__c</errorConditionFormula>
        <errorMessage>The quantity shipped cannot be more than the quantity allocated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Both_Purchasing_And_Manufacturing</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Manufacturing_Order__c) ),  NOT(ISBLANK(Purchase_Order_Line__c )))</errorConditionFormula>
        <errorDisplayField>Product__c</errorDisplayField>
        <errorMessage>A product can not be on back order as a manufactured item and a purchased item.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Ordered_Greater_Equal_Allocated</fullName>
        <active>true</active>
        <description>the quantity ordered is greater than or equal to the quantity allocated</description>
        <errorConditionFormula>Quantity_Allocated__c &gt; Quantity_Ordered__c</errorConditionFormula>
        <errorDisplayField>Quantity_Ordered__c</errorDisplayField>
        <errorMessage>The quantity ordered cannot be less than the quantity allocated</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Product_Is_Required</fullName>
        <active>true</active>
        <description>product is required</description>
        <errorConditionFormula>ISBLANK(Product__c)</errorConditionFormula>
        <errorDisplayField>Product__c</errorDisplayField>
        <errorMessage>You must enter a value</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Quantity_Is_Positive</fullName>
        <active>true</active>
        <description>quantity must be greater than or equal to 0</description>
        <errorConditionFormula>Quantity_Ordered__c &lt; 0</errorConditionFormula>
        <errorDisplayField>Quantity_Ordered__c</errorDisplayField>
        <errorMessage>quantity must be greater than or equal to 0</errorMessage>
    </validationRules>
