    <fields>
        <fullName>Time_Card_Period__c</fullName>
        <deleteConstraint>Restrict</deleteConstraint>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <inlineHelpText>Used for grouping of Time Card Lines. Currently only 7 day Time card periods are supported.</inlineHelpText>
        <label>Time Card Period</label>
        <lookupFilter>
            <active>false</active>
            <errorMessage>This Time Period has been Closed. Please contact your administrator to make any changes to this period.</errorMessage>
            <filterItems>
                <field>Time_Card_Period__c.Status__c</field>
                <operation>notEqual</operation>
                <value>Closed</value>
            </filterItems>
            <isOptional>false</isOptional>
        </lookupFilter>
        <referenceTo>Time_Card_Period__c</referenceTo>
