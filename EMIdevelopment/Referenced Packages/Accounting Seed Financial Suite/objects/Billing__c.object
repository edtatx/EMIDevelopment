    <fields>
        <fullName>Age__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(AND(TODAY() - Due_Date2__c &lt;= 0,(ROUND(Balance__c, 2) &gt; 0)), &quot;0-Current&quot;,
IF(AND(30 &gt;= TODAY() - Due_Date2__c, TODAY() - Due_Date2__c &gt; 0, (ROUND(Balance__c, 2) &gt; 0)), &quot;1-30 Days&quot;,
IF(AND(60 &gt;= TODAY() - Due_Date2__c, TODAY() - Due_Date2__c &gt; 0, (ROUND(Balance__c, 2) &gt; 0)), &quot;31-60 Days&quot;,
IF(AND(90 &gt;= TODAY() - Due_Date2__c, TODAY() - Due_Date2__c &gt; 0, (ROUND(Balance__c, 2) &gt; 0)), &quot;61-90 Days&quot;,
IF(AND(TODAY() - Due_Date2__c &gt; 90, (ROUND(Balance__c, 2) &gt; 0)), &quot;Over 90 Days&quot;,
IF(AND(ROUND(Balance__c, 2) = 0, TEXT(Status__c) = &quot;Posted&quot;), &quot;Closed&quot;,
&quot;&quot;))))))</formula>
        <inlineHelpText>Measures the current age of the balance of the billing. Used to monitor collection of balance.</inlineHelpText>
        <label>Age</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
